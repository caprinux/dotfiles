configuration {
	modi: "drun";
	font: "Cascadia Code 10";
	show-icons: true;
	run-command: "{cmd}";
	run-shell-command: "{terminal} -e {cmd}";
	icon-theme: "Tela-Circle-Purple";
	drun-url-launcher: "xdg-open";
	dpi: 1;
/*	theme: ;*/
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}
@theme "/home/cap/.config/rofi/themes/slate.rasi"
